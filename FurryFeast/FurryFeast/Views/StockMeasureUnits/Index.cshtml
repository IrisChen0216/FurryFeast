@model IEnumerable<FurryFeast.Models.StockMeasureUnit>

@{
    ViewData["Title"] = "Index";
}

<h1>StockMeasureUnits</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MeasureUnitsCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MeasureUnitsDescription)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
